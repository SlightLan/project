<h1>Here is question page</h1>
<table>
	<tr>
		<th>Title</th>
		<th>Content</th>
		<th>Asker</th>
	</tr>

	<% @populars.each do |t| %>
		<tr>
			<td><%= t.title %></td>
			<td><%= t.content %></td>
			<td><%= t.asker %></td>
			<td><%= link_to 'show this question', t %></td>
		</tr>
	<% end %>
</table>
